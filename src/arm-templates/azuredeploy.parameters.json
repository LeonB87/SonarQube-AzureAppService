{
    "$schema": "https://schema.management.azure.com/schemas/2019-04-01/deploymentParameters.json#",
    "contentVersion": "1.0.0.0",
    "parameters": {
        "hostingPlanName": {
            "value": "#(|SonarQubeAppServicePlan|)#"
        },
        "location": {
            "value": "#(|location|)#"
        },
        "siteName": {
            "value": "#(|sonarqubeSiteName|)#"
        },
        "skuName": {
            "value": "#(|appserviceSkuName|)#"
        },
        "repoUrl": {
            "value": "https://github.com/LeonB87/SonarQube-AzureAppService"
        },
        "branch": {
            "value": "#(|deploymentBranch|)#"
        },
        "SonarQube Edition": {
            "value": "#(|sonarqubeEdition|)#"
        },
        "SonarQube Version": {
            "value": "#(|sonarqubeVersion|)#"
        },
        "taggingEnvironment": {
            "value": "#(|taggingEnvironment|)#"
        },
        "databaseName": {
            "value": "#(|databaseName|)#"
        },
        "sqlAdministratorLogin": {
            "value": "#(|sqlAdministratorLogin|)#"
        },
        "sqlServerName": {
            "value": "#(|sqlServerName|)#"
        },
        "sqlAdministratorLoginPassword": {
            "value": "#(|sqlAdministratorLoginPassword|)#"
        }
    }
}